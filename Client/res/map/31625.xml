		<!-- <proAction id="1" type="8" point="100,100" immediately="0" to="" /> -->
		<!-- 入场 -->
		<proAction npcID="30" id="1" type="0" answer="@blank" to="a2" colorID="2"  >
			<des>
				<![CDATA[1#【@nick】等你很久了！]]>
			</des>
		</proAction>
		<proAction npcID="24" id="2" type="0" answer="@blank" to="onChangeBgm" colorID="2"  >
			<des>
				<![CDATA[2#肃静！现在摩尔王继承仪式正式开始！]]>
			</des>
		</proAction>
		<!-- 切换背景音乐 -->
		<proAction npcID="24" id="3" type="0" answer="@blank" to="onPlayApplause1" colorID="2"  >
			<des>
				<![CDATA[1#欢迎我们智慧与实力并肩的，第十三勇士。兼现任摩尔王继承人——【@nick】入场。]]>
			</des>
		</proAction>
		<!-- 掌声 -->
		<proAction npcID="10000" id="4" type="0" answer="@blank" to="onState1" colorID="2"  >
			<des>
				<![CDATA[1#什么？？？？我是在做梦吗？]]>
			</des>
		</proAction>
		<!-- state1 -->
		<proAction npcID="1" id="5" type="0" answer="@blank" to="a6" colorID="2"  >
			<des>
				<![CDATA[1#嘿嘿，这才是我信赖的好朋友。]]>
			</des>
		</proAction>
		<proAction npcID="6" id="6" type="0" answer="@blank" to="a7" colorID="2"  >
			<des>
				<![CDATA[1#【@nick】实在是太厉害了，你是大家的榜样。]]>
			</des>
		</proAction>
		<proAction npcID="5" id="7" type="0" answer="大家别说了我害羞TT$低调低调$不要迷恋我" to="onState2$onState2$onState2" colorID="2$2$2"  >
			<des>
				<![CDATA[1#嚯嚯，刚冒险的时候还是我教【@nick】的技能。]]>
			</des>
		</proAction>
		<!-- state2 -->
		<!-- 龙 -->
		<proAction npcID="20602" id="8" type="0" answer="@blank" to="a9" colorID="2"  >
			<des>
				<![CDATA[1#【@nick】大人救了我的孩子，他是最善良的王者。]]>
			</des>
		</proAction>
		<proAction npcID="329" id="9" type="0" answer="连龙族都来了！$这到底啥情况$风之龙快来当主战！" to="onState3$onState3$onState3" colorID="2$2$2"  >
			<des>
				<![CDATA[1#【@nick】大人，吾愿意每天在您旁边当电风扇。]]>
			</des>
		</proAction>
		<!-- state3 -->
		<!-- 导师 -->
		<proAction npcID="26" id="10" type="0" answer="@blank" to="a11" colorID="2"  >
			<des>
				<![CDATA[1#真不愧是我看中的摩尔，言行举止都能迷倒万千少女。]]>
			</des>
		</proAction>
		<proAction npcID="28" id="11" type="0" answer="@blank" to="a12" colorID="2"  >
			<des>
				<![CDATA[2#何等敏锐的目光，这就是新的王者啊。]]>
			</des>
		</proAction>
		<proAction npcID="27" id="12" type="0" answer="@blank" to="a13" colorID="2"  >
			<des>
				<![CDATA[2#这澎湃的魔力，你身上带有星辰的眷顾。]]>
			</des>
		</proAction>
		<proAction npcID="22" id="13" type="0" answer="@blank" to="a14" colorID="2"  >
			<des>
				<![CDATA[1#愿圣光赐福，为你铺就王者之路。]]>
			</des>
		</proAction>
		<proAction npcID="29" id="14" type="0" answer="感谢老师们的教诲$不会忘记自己的使命的！$yeah！" to="onState4$onState4$onState4" colorID="2$2$2"  >
			<des>
				<![CDATA[1#成了国王也不要忘记修炼啊！]]>
			</des>
		</proAction>
		<!-- state4 -->
		<!-- 魔王 -->
		<proAction npcID="337" id="15" type="0" answer="@blank" to="a16" colorID="2"  >
			<des>
				<![CDATA[1#我，伊恩大魔王——的影子承认你是我伟大的契约者。]]>
			</des>
		</proAction>
		<proAction npcID="345" id="16" type="0" answer="@blank" to="a17" colorID="2"  >
			<des>
				<![CDATA[1#我愿意与【@nick】签订契约，为您做牛做马。]]>
			</des>
		</proAction>
		<proAction npcID="350" id="17" type="0" answer="魔王们？！$正确的选择$臣服于我才是正道！" to="onState5$onState5$onState5" colorID="2$2$2"  >
			<des>
				<![CDATA[1#【@nick】大人，这是我给您献上的财宝！]]>
			</des>
		</proAction>
		<!-- state5 -->
		<!-- 勇士 -->
		<proAction npcID="104" id="18" type="0" answer="@blank" to="a19" colorID="2"  >
			<des>
				<![CDATA[1#我就知道你能成为摩尔王。]]>
			</des>
		</proAction>
		<proAction npcID="101" id="19" type="0" answer="@blank" to="a20" colorID="2"  >
